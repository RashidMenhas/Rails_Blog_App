<% content_for :title, "Blog App" %>
<section class="users-container">
  <% @user.each do |user| %>
    <%= render "user", user: user %>
  <% end %>

  <div class="create-post">
    <%= link_to new_user_post_path(@user) do %>
      <button class="btn-create-post">create post</button>
    <%end%>
  </div>
</section>